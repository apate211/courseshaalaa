<div class="container p-3 m-3">
    <table id="example" class="table table-dark table-striped" style="width:100%">
    
        <thead>
            <tr>
                <th> Assignments </th>
                <th> Title </th>
                <th> submissions</th>
            </tr>
        </thead>
        <tbody>
            {{#each assignments}}
                <tr>
                    <td> Assignment {{this.sequencenumber}}</td>
                    <td> {{this.title}}</td>
                    <td> <button class="btn btn-success" > view submissions </button></td>
                </tr>
            {{/each}}
        </tbody>
    
    </table>
</div>

